<head>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">

    <!-- jQuery library -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>

    <!-- Popper JS -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>

    <!-- Latest compiled JavaScript -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
    <style>
        body{
            z-index: 0;
            background-image: url('data:image/jpeg;base64,');
            background-repeat: no-repeat;
            background-size: cover;
        }
    </style>
</head>
<body>
    <br />
    <br />
    <div class="container mt-5">
        
        <% if(arr.length>=3){ %>
            <div class="row">
                <% for(let i=0;i<arr.length;i++){ %>
                    <div class="col-sm-12 col-md-6 col-lg-4 mt-2 mb-5">
                        <div class="card" style="border-radius: 5px; z-index: 24;">
                            <div class="card-header" style="text-align: center;">
                                <span class="px-2 pt-3" style="font-size: 16.5px;"><b><%= arr[i].title %>(<%= arr[i].start %>)</b></span>
                                <p class="card-subtitle mt-2 text-muted" style="text-align: center; z-index: 100;"><i>- Client: <%= d[arr[i].client_id] %></i></p>
                            </div>
                            <% if(arr[i].description.length>200){ %>
                                <div class="card-body pb-3 px-4" style="color: black;"><b><span style="color:rgba(253, 80, 55, 0.822)">Deadline: <%= arr[i].deadline %></span></b><p><%= arr[i].description.slice(0,200)+'...' %></p></div>
                            <% } %>
                            <% if(arr[i].description.length<=200){ %>
                                <div class="card-body" style="color: black;"><b><span style="color:rgba(253, 80, 55, 0.822)">Deadline: <%= arr[i].deadline %></span></b><p><%= arr[i].description %></p></div>
                            <% } %>
                            <div class="card-footer p-4"style="text-align: center;"><a href='/viewclientproject/<%= arr[i].project_id %>' class='btn btn-outline px-4 py-1' style="background-color: #049161; color: #fff;">View</a></div>
                        </div>
                    </div>
                <% } %>
            </div>
        <% } %>
        <% if(arr.length===2){ %>
            <div class="row">
                <div class="col-sm-0 col-md-0 col-lg-2 mt-2 mb-5"></div>
                <% for(let i=0;i<arr.length;i++){ %>
                    <div class="col-sm-12 col-md-6 col-lg-4 mt-2 mb-5">
                        <div class="card" style="border-radius: 5px; z-index: 24;">
                            <div class="card-header" style="text-align: center;">
                                <span class="px-2 pt-3" style="font-size: 16.5px;"><b><%= arr[i].title %> (<%= arr[i].start %>)</b></span>
                                <p class="card-subtitle mt-2 text-muted" style="text-align: center; z-index: 100;"><i>- Client: <%= d[arr[i].client_id] %></i></p>
                            </div>
                            <% if(arr[i].description.length>200){ %>
                                <div class="card-body pb-3 px-4"><b><span style="color:rgba(253, 80, 55, 0.822)">Deadline: <%= arr[i].deadline %></span></b><p class="mt-2"><%= arr[i].description.slice(0,200)+'...' %></p></div>
                            <% } %>
                            <% if(arr[i].description.length<=200){ %>
                                <div class="card-body"><b><span style="color:rgba(253, 80, 55, 0.822)">Deadline: <%= arr[i].deadline %></span></b><p class='mt-2'><%= arr[i].description %></p></div>
                            <% } %>
                            <div class="card-footer p-4"style="text-align: center;"><a href='/viewclientproject/<%= arr[i].project_id %>' class='btn btn-outline px-4 py-1' style="background-color: #049161; color: #fff;">View</a></div>
                        </div>
                    </div>
                <% } %>
                <div class="col-sm-0 col-md-0 col-lg-2 mt-2 mb-5"></div>
            </div>
        <% } %>
        <% if(arr.length===1){ %>
            <div class="row">
                <div class="col-sm-0 col-md-3 col-lg-4 mt-2 mb-5"></div>
                <% for(let i=0;i<arr.length;i++){ %>
                    <div class="col-sm-12 col-md-6 col-lg-4 mt-2 mb-5">
                        <div class="card" style="border-radius: 5px; z-index: 24;">
                            <div class="card-header" style="text-align: center;">
                                <span class="px-2 pt-3" style="font-size: 16.5px;"><b><%= arr[i].title %> (<%= arr[i].start %>)</b></span>
                                <p class="card-subtitle mt-2 text-muted" style="text-align: center; z-index: 100;"><i>- Client: <%= d[arr[i].client_id] %></i></p>
                            </div>
                            <% if(arr[i].description.length>200){ %>
                                <div class="card-body pb-3 px-4"><b><span style="color:rgba(253, 80, 55, 0.822)">Deadline: <%= arr[i].deadline %></span></b><%= arr[i].description.slice(0,200)+'...' %></div>
                            <% } %>
                            <% if(arr[i].description.length<=200){ %>
                                <div class="card-body"><b><span style="color:rgba(253, 80, 55, 0.822)">Deadline: <%= arr[i].deadline %></span></b><p><%= arr[i].description %></p></div>
                            <% } %>
                            <div class="card-footer p-4"style="text-align: center;"><a href='/viewclientproject/<%= arr[i].project_id %>' class='btn btn-outline px-4 py-1' style="background-color: #049161; color: #fff;">View</a></div>
                        </div>
                    </div>
                <% } %>
                <div class="col-sm-0 col-md-3 col-lg-4 mt-2 mb-5"></div>
            </div>
        <% } %>
        <% if(arr.length===0){ %>
            <body style="background-color: white; background-image: url('');">
                <span style="font-size: 30px; font-weight: 500;">No new projects</span>  <img src='data:image/jpeg;base64,' width="35" height="40" style="position: relative; top: -5px;" />
                <hr />
            </body>
        <% } %>
        <br />
        <br />
        <br />
    </div>
</body>